class MyQueue {
public:
    MyQueue() {

    }
    
    void push(int x) {
        s2.push(x);
    }
    
    int pop() {
        if(!s1.empty()){
            int result=s1.top();
            s1.pop();
            return result;
        }
        while(!s2.empty()){
            int tmp=s2.top();
            s2.pop();
            s1.push(tmp);
        }
        int result=s1.top();
        s1.pop();
        return result;
    }
    
    int peek() {
        if(!s1.empty()){
            return s1.top();
        }
        while(!s2.empty()){
            int tmp=s2.top();
            s2.pop();
            s1.push(tmp);
        }
        return s1.top();
    }
    
    bool empty() {
        return s1.empty()&&s2.empty();
    }
private:
    stack<int> s1,s2;
};

/**
 * Your MyQueue object will be instantiated and called as such:
 * MyQueue* obj = new MyQueue();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->peek();
 * bool param_4 = obj->empty();
 */
